{
  "parser": "babel-eslint",
  "extends": ["eslint-config-uber-jsx", "eslint-config-uber-es2015"],
  "plugins": ["babel"],
  "rules": {
    "eol-last": 1,
    "prefer-const": 1,
    "complexity": 0,
    "operator-linebreak": 0,
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],

    "func-style": 0,
    "max-len": 0,
    "no-var": 0,
    "prefer-spread": 0,
    "prefer-template": 0,
    "strict": 0,
    "object-curly-spacing": [2, "never"],
    "valid-jsdoc": 0,
    "max-statements": ["warn", 35],
    "no-invalid-this": 0,
    "react/no-multi-comp": 0,
    "react/sort-comp": 0,
    "no-inline-comments": 0,
    "quote-props": 0,
    "consistent-return": 0,
    "max-params": 0,
    "max-nested-callbacks": 0
  }
}
